{
    "manifest_version": 3,
    "name": "Solo Texto",
    "description": "Una extensión para mostrar solo el texto de una página web.",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting"
      ],
      
    "action": {
      "default_title": "Solo Texto",
      "default_popup": "popup.html"
    }
  }
